{
  "service_name": "CreatorVerse AI Token Metrics Service",
  "environ": "development",
  "kafka": {
    "bootstrap_servers": "localhost:9092",
    "topic": "creatorverse.ai.token_usage",
    "group_id": "token-metrics-consumer",
    "security_protocol": "PLAINTEXT"
  },
  "clickhouse": {
    "host": "localhost",
    "port": 9000,
    "database": "creatorverse",
    "username": "default",
    "password": ""
  },
  "batch": {
    "size": 100,
    "timeout_ms": 5000
  },
  "health_check_port": 8080,
  "log_level": "info"
}
